            </dependency>
            <dependency>
                <groupId>org.tiankafei.tool</groupId>
                <artifactId>tiankafei-ui-control</artifactId>
                <version>${org.tiankafei.tool.version}</version>
            </dependency>
            <dependency>
                <groupId>org.tiankafei.tool</groupId>
                <artifactId>tiankafei-ui-chart</artifactId>
                <version>${org.tiankafei.tool.version}</version>
            </dependency>

