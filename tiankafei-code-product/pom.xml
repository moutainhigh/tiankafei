<?xml version="1.0" encoding="UTF-8"?>

<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.tiankafei</groupId>
        <artifactId>tiankafei-parent</artifactId>
        <version>1.0-RELEASE</version>
    </parent>

    <groupId>org.tiankafei.product</groupId>
    <artifactId>tiankafei-code-product</artifactId>
    <version>1.0-RELEASE</version>
    <packaging>pom</packaging>

    <description>此工程是tiankafei的产品包集合</description>
    <url>https://github.com/tiankafei/tiankafei</url>

    <modules>
        <module>tiankafei-register-center</module>
        <module>tiankafei-config-center</module>
        <module>tiankafei-monitor-center</module>
        <module>tiankafei-message-center</module>
        <module>tiankafei-micro-depends-center</module>
        <module>tiankafei-cache-center</module>
        <module>tiankafei-api-gateway</module>
        <module>tiankafei-frame-start</module>
        <module>tiankafei-code-generator</module>
        <module>tiankafei-distributed-id-center</module>
        <module>tiankafei-distributed-lock-center</module>
        <module>tiankafei-distributed-scheduling-center</module>
        <module>tiankafei-service</module>
    </modules>

    <properties>
        <org.tiankafei.tool.version>1.4.1-RELEASE</org.tiankafei.tool.version>
        <org.tiankafei.produce.version>1.0-RELEASE</org.tiankafei.produce.version>
    </properties>

    <dependencyManagement>
        <dependencies>
            <!--自己的工具包-->
            <dependency>
                <groupId>org.tiankafei.tool</groupId>
                <artifactId>tiankafei-base</artifactId>
                <version>${org.tiankafei.tool.version}</version>
            </dependency>
            <dependency>
                <groupId>org.tiankafei.tool</groupId>
                <artifactId>tiankafei-aviator-extend</artifactId>
                <version>${org.tiankafei.tool.version}</version>
            </dependency>

            <!--产品自己的工具包-->
            <!--微服务依赖的工程-->
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-micro-depends</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <!--注册中心依赖的工程-->
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-register-depends</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <!--公共代码工程-->
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-code-common</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <!--用户公共属性工程-->
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-user-common</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <!--用户缓存工程-->
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-user-cache</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <!--通用的数据库查询工程-->
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-db</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <!--公共采集控件工程-->
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-collection</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>

            <!--feign调用客户端工程-->
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-user-api</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-login-api</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>

            <!--用户登录相关工程-->
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-login-common</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>

            <!--缓存工程-->
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-cache</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-cache-redis</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-cache-memory</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>

            <!--消息MQ工程-->
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-message</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-message-disruptor</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-message-kafka</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-message-rabbitmq</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-message-redis</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-message-rocketmq</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>

            <!--分布式锁工程-->
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-distributed-lock</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-distributed-lock-etcd</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-distributed-lock-mysql</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-distributed-lock-redis</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
            <dependency>
                <groupId>org.tiankafei.product</groupId>
                <artifactId>tiankafei-distributed-lock-zookeeper</artifactId>
                <version>${org.tiankafei.produce.version}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>

</project>
