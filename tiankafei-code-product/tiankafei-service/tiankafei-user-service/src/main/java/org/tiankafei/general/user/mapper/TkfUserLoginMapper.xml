<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.tiankafei.general.user.mapper.TkfUserLoginMapper">

            <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, username, email, telephone, password, status, expiration_date, create_time
    </sql>

    <select id="getTkfUserLoginList" resultType="org.tiankafei.general.user.vo.TkfUserLoginQueryVo">
        select
        <include refid="Base_Column_List"/>
        from tkf_user_login
    </select>

</mapper>
