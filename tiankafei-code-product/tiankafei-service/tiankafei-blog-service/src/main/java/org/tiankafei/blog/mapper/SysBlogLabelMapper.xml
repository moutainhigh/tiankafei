<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.tiankafei.blog.mapper.SysBlogLabelMapper">

            <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, count, create_time, create_user_id, update_time, update_user_id
    </sql>

    <select id="getSysBlogLabelList" resultType="org.tiankafei.blog.vo.SysBlogLabelQueryVo">
        select
        <include refid="Base_Column_List"/>
        from sys_blog_label
    </select>

</mapper>
